{
  "name": "eisbuk-admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/luxon": "2.10.11",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.2.10",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "firebase": "^8.2.3",
    "firebase-tools": "^9.1.1",
    "formik": "^2.1.5",
    "formik-material-ui": "^3.0.1",
    "luxon": "^1.25.0",
    "moment": "^2.27.0",
    "notistack": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-firebaseui": "^5.0.2",
    "react-redux": "^7.2.1",
    "react-redux-firebase": "^3.9.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.0.5",
    "redux-auth-wrapper": "^3.0.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-firestore": "^0.15.0",
    "redux-thunk": "^2.3.0",
    "typescript": "~4.3.4",
    "unix-timestamp": "^0.2.0",
    "uuid": "^8.3.0",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "sed -e \"s/Date this was last changed:.*/Date this was last changed: $(date)/\" -i src/config/envInfoEnv.js ; react-app-rewired start",
    "start:eisbuk": "sed -e \"s/Date this was last changed:.*/Date this was last changed: $(date)/\" -i src/config/envInfoEnv.js ; EISBUK_SITE=eisbuk.web.app react-app-rewired start --reset-cache",
    "start:igorice": "sed -e \"s/Date this was last changed:.*/Date this was last changed: $(date)/\" -i src/config/envInfoEnv.js ;EISBUK_SITE=igorice.web.app react-app-rewired start --reset-cache",
    "emulators:inspect": "exec firebase emulators:start --project eisbuk --inspect-functions 9229",
    "emulators:start": "exec firebase emulators:start --project eisbuk",
    "build": "react-app-rewired build",
    "test": "firebase -c firebase-testing.json  emulators:exec --project eisbuk -- 'yarn run jest --watch --runInBand'",
    "test:ci": "firebase -c firebase-testing.json  emulators:exec --project eisbuk -- 'yarn run jest --forceExit --runInBand'",
    "deploy:eisbuk": "firebase deploy --project eisbuk --only hosting:eisbuk",
    "deploy:production": "firebase deploy --project eisbuk --only hosting:igorice; firebase deploy --project eisbuk --except hosting",
    "postinstall": "sh fix-jsdom.sh",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "chromatic": "npx chromatic --project-token dcmb4kmorxe"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-unused-vars": [
        "error"
      ]
    },
    "settings": {
      "react": {
        "version": "16.13.1"
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.12.10",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "chromatic": "^5.6.0",
    "customize-cra": "^1.0.0",
    "debugging-aid": "^0.5.3",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-unused-imports": "^1.0.1",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.11.0",
    "mocha": "^9.0.1",
    "plop": "^2.7.4",
    "react-app-rewired": "^2.1.8",
    "reselect": "^4.0.0",
    "seedrandom": "^3.0.5"
  }
}
